# Production Environment Configuration for Deployed Frontend
# This file is used when frontend is deployed (e.g., on Vercel, Netlify)

# Aptos Network Configuration
REACT_APP_APTOS_NETWORK=devnet
REACT_APP_APTOS_NODE_URL=https://fullnode.devnet.aptoslabs.com/v1

# Backend Service Configuration - Production
REACT_APP_BACKEND_URL_PRODUCTION=https://aptos-aura-weaver.up.railway.app
REACT_APP_BACKEND_URL_DEVELOPMENT=http://localhost:3001

# IPFS Configuration
REACT_APP_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/

# Feature Flags for Production
REACT_APP_ENABLE_IPFS=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_DEBUG_MODE=false
REACT_APP_ENABLE_MOCK_DATA=false